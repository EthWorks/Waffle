{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "quotemark": [true, "single"],
    "ordered-imports": false,
    "trailing-comma": [true, {
      "multiline": "never",
      "singleline": "never"
    }],
    "interface-name": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "member-ordering": false,
    "array-type": [true, "array"]
  }
}